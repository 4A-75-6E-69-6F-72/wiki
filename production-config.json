{
  "index_name": "prod_docs_wiki",
  "start_urls": ["https://wiki.jmonkeyengine.org/"],
  "selectors": {
    "default": {
      "lvl0": {
        "global": true,
        "selector": ".navbar .component"
      },
      "lvl1": ".doc > h1.page",
      "lvl2": ".doc .sect1 > h2:first-child",
      "lvl3": ".doc .sect2 > h3:first-child",
      "lvl4": ".doc .sect3 > h4:first-child",
      "text": ".doc p, .doc dt"
    }
  },
  "min_indexed_level": 2,
}
